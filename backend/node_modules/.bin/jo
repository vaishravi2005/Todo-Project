#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/iojs" ]; then
  exec "$basedir/iojs" --harmony --es_staging "$basedir/../jo/bin/jo" "$@"
else 
  exec iojs --harmony --es_staging "$basedir/../jo/bin/jo" "$@"
fi
